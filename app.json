{
  "expo": {
    "name": "I-am-a-Developer",
    "slug": "I-am-a-Developer",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBF5F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/loading.png",
        "backgroundColor": "#FBF5F2",
        "scaleType": "center"
      }
    },
    "web": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/loading.png",
        "backgroundColor": "#FBF5F2",
        "scaleType": "center"
      }
    }
  }
}